tDefault()\r\n        // Reset our form values\r\n        this.form.email = ''\r\n        this.form.name = ''\r\n        this.form.food = null\r\n        this.form.checked = []\r\n        // Trick to reset/clear native browser form validation state\r\n        this.show = false\r\n        this.$nextTick(() => {\r\n          this.show = true\r\n        })\r\n      },\r\n      navigate() {\r\n            this.$router.go(-1);\r\n      }\r\n    },\r\n    computed: {\r\n      entries() {\r\n        return Blogs\r\n      },\r\n      date() {\r\n        return Object.keys(Blogs)[0];\r\n      }\r\n    },\r\n}\r\n\r\n",{"version":3,"sources":["contact.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyGA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"contact.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div>\r\n    <header class=\"showcase\">\r\n        <div class=\"container showcase-inner\">\r\n        <h1>Welcome</h1>\r\n        <p>maiores sint impedit delectus quam molestiae explicabo cum facere ratione veritatis.</p>\r\n\r\n        <div>\r\n          <input type=\"checkbox\" class=\"toggler\">\r\n\r\n              <input type=\"checkbox\" id=\"html\" value=\"HTML\" class=\"toggler\">\r\n              <label for=\"html\"><a class=\"btn\">Read More</a></label>\r\n\r\n          <div class=\"hamburger\"><div>&#10007;</div></div>\r\n        <div class=\"menu\">\r\n        <div>\r\n            <div>\r\n              <ul>\r\n                  <li><a href=\"#\">Home</a></li>\r\n                  <li><a href=\"#\">About</a></li>\r\n                  <li><a href=\"#\">Services</a></li>\r\n                  <li><a href=\"#\">Contact</a></li>\r\n              </ul>\r\n            </div>\r\n        </div>\r\n        </div>\r\n\r\n        </div>\r\n        </div>\r\n    </header>\r\n\r\n      <h3 class=\"center\">{{date}}</h3>\r\n        <div class=\"section\" v-for=\"entry in entries[date]\" :key=\"entry.id\">\r\n          <div class=\"entry\">\r\n            <h3 @click=\"$router.push({name: entry.id})\">\r\n              {{entry.title}}\r\n              <span class=\"subtitle\">:{{entry.date}}</span>\r\n            </h3>\r\n            <p class=\"resto\">{{entry.description}}</p>\r\n          </div>\r\n        </div>\r\n\r\n<br>\r\n<!-- ok so, here we start with the form for the contact -->\r\n\r\n  <div>\r\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n      <b-form-group\r\n        id=\"input-group-1\"\r\n        label=\"Email address:\"\r\n        label-for=\"input-1\"\r\n        description=\"We'll never share your email with anyone else.\"\r\n      >\r\n        <b-form-input\r\n          id=\"input-1\"\r\n          v-model=\"form.email\"\r\n          type=\"email\"\r\n          required\r\n          placeholder=\"Enter email\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-2\" label=\"Your Name:\" label-for=\"input-2\">\r\n        <b-form-input\r\n          id=\"input-2\"\r\n          v-model=\"form.name\"\r\n          required\r\n          placeholder=\"Enter name\"\r\n        ></b-form-input>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-3\" label=\"Food:\" label-for=\"input-3\">\r\n        <b-form-select\r\n          id=\"input-3\"\r\n          v-model=\"form.food\"\r\n          :options=\"foods\"\r\n          required\r\n        ></b-form-select>\r\n      </b-form-group>\r\n\r\n      <b-form-group id=\"input-group-4\">\r\n        <b-form-checkbox-group v-model=\"form.checked\" id=\"checkboxes-4\">\r\n          <b-form-checkbox value=\"me\">Check me out</b-form-checkbox>\r\n          <b-form-checkbox value=\"that\">Check that out</b-form-checkbox>\r\n        </b-form-checkbox-group>\r\n      </b-form-group>\r\n\r\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\r\n      <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\r\n    </b-form>\r\n    <b-card class=\"mt-3\" header=\"Form Data Result\">\r\n      <pre class=\"m-0\">{{ form }}</pre>\r\n    </b-card>\r\n  </div>\r\n\r\n\r\n\r\n    <button type=\"button\" class=\"btn btn-primary\" v-on:click=\"navigate()\"> \r\n        minus 2\r\n    </button>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Blogs from '../statics/blogs.json'\r\n\r\nexport default {\r\n  name: 'contact',\r\n    data() {\r\n      return {\r\n        form: {\r\n          email: '',\r\n          name: '',\r\n          food: null,\r\n          checked: []\r\n        },\r\n        foods: [{ text: 'Select One', value: null }, 'Carrots', 'Beans', 'Tomatoes', 'Corn'],\r\n        show: true\r\n      }\r\n    },\r\n    methods: {\r\n      onSubmit(evt) {\r\n        evt.preventDefault()\r\n        alert(JSON.stringify(this.form))\r\n      },\r\n      onReset(evt) {\r\n        evt.preventDefault()\r\n        // Reset our form values\r\n        this.form.email = ''\r\n        this.form.name = ''\r\n        this.form.food = null\r\n        this.form.checked = []\r\n        // Trick to reset/clear native browser form validation state\r\n        this.show = false\r\n        this.$nextTick(() => {\r\n          this.show = true\r\n        })\r\n      },\r\n      navigate() {\r\n            this.$router.go(-1);\r\n      }\r\n    },\r\n    computed: {\r\n      entries() {\r\n        return Blogs\r\n      },\r\n      date() {\r\n        return Object.keys(Blogs)[0];\r\n      }\r\n    },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n@import '../css/front.css';\r\n@import url('https://fonts.googleapis.com/css?family=Chilanka&display=swap');\r\n\r\n.hamburger {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 1;\r\n  width: 60px;\r\n  height: 60px;\r\n  padding: 0.5rem;\r\n  background: red;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  opacity: 0;\r\n}\r\n\r\n.showcase {\r\n  background-color: rgba(13, 110, 139, 0.75);\r\n  color: #fff;\r\n  height: 100vh;\r\n  position: relative;\r\n}\r\n\r\n.showcase:before {\r\n  content: '';\r\n  background: url('../assets/bdd.png') no-repeat center center/cover;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: -1;\r\n}\r\n\r\n.showcase .showcase-inner {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  text-align: center;\r\n  height: 100%;\r\n}\r\n\r\n.showcase h1 {\r\n  font-size: 4rem;\r\n}\r\n\r\n.showcase p {\r\n    font-family: 'Chilanka', cursive;\r\n}\r\n\r\n.btn {\r\n  display: inline-block;\r\n  border: none;\r\n  background: rgba(24, 39, 51 , 0.85);\r\n  color: #fff;\r\n  padding: 0.75rem 1.5rem;\r\n  margin-top: 1rem;\r\n  transition: opacity 0.5s ease;\r\n  text-decoration: none;\r\n}\r\n\r\n.btn:hover {\r\n  opacity: 0.65;\r\n  background-color: rgb(19, 39, 54);\r\n}\r\n\r\n.toggler {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 2;\r\n  cursor: pointer;\r\n  width: 50px;\r\n  height: 50px;\r\n  opacity: 0;\r\n}\r\n\r\n.menu {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  visibility: hidden;\r\n  overflow: hidden;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.menu > div {\r\n  background: rgba(24, 39, 51 , 0.85);\r\n  border-radius: 50%;\r\n  width: 200vw;\r\n  height: 200vw;\r\n  display: flex;\r\n  flex: none;\r\n  align-items: center;\r\n  justify-content: center;\r\n  transform: scale(0);\r\n  transition: all 0.4s ease;\r\n}\r\n\r\n.menu > div > div {\r\n  text-align: center;\r\n  max-width: 90vw;\r\n  max-height: 100vh;\r\n  opacity: 0;\r\n  transition: opacity 0.4s ease;\r\n}\r\n\r\n.menu > div > div > ul > li {\r\n  list-style: none;\r\n  color: #fff;\r\n  font-size: 1.5rem;\r\n  padding: 1rem;\r\n}\r\n\r\n.menu > div > div > ul > li > a {\r\n  color: inherit;\r\n  text-decoration: none;\r\n  transition: color 0.4s ease;\r\n}\r\n\r\n.toggler:checked ~ .menu {\r\n  visibility: visible;\r\n}\r\n\r\n.toggler:checked ~ .hamburger {\r\n  opacity: 1;\r\n}\r\n\r\n.toggler:checked ~ .menu > div {\r\n  transform: scale(1);\r\n  transition-duration: 0.75s;\r\n}\r\n\r\n.toggler:checked ~ .menu > div > div {\r\n  opacity: 1;\r\n  transition:  opacity 0.4s ease 0.4s;\r\n}\r\n\r\n.section{\r\n    text-align: center;\r\n}\r\n\r\n.entry{\r\n    margin-top: 10px;\r\n    margin-bottom: 10px;\r\n    height: 50px;\r\n}\r\n\r\n.subtitle{\r\n    margin-bottom: 10px;\r\n    color: white;\r\n    font-family: 'Chilanka', cursive;\r\n}\r\n.resto{\r\n    margin-bottom: 30px;\r\n}\r\n</style>"]}]}